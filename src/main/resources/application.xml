<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
	   http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<!--<context:annotation-config/>-->

	<beans>

		<bean id="dPersistor" class="org.devocative.samples.spring.bean.DPersistor"
			  init-method="init" destroy-method="shutdown">
			<property name="key1" value="#{dConfig.getString('key1')}"/>
			<property name="key2" value="#{dConfig.getString('key2')}"/>
		</bean>

		<bean id="dConfig" class="org.devocative.samples.spring.bean.DConfig"/>

	</beans>

</beans>
